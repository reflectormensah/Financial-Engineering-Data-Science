        "# Describing the dataset\n",
        "tickers = [\"ISFD.L\", \"IGLN.L\"]\n",
        "\n",
        "start = dt.date(2018, 10, 18)\n",
        "end = dt.date(2022, 10, 18)\n",
        "\n",
        "df = yf.download(tickers, start, end, interval=\"1d\")['Adj Close']\n",
        "df[[\"ISFD.L_ret\", \"IGLN.L_ret\"]] = np.log(df) - np.log(df.shift(1))\n",
        "df = df.iloc[1:, 2:]"
      ]
