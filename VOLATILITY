        "# Pct change\n",
        "pct_daily_return = 100 * df.mean()\n",
        "pct_daily_return = pd.DataFrame(pct_daily_return, columns=[\"PctDailyReturns\"])\n",
        "\n",
        "# Standard deviation\n",
        "std_dev = df.std()\n",
        "std_dev = pd.DataFrame(std_dev, columns=[\"StandardDeviation\"])\n",
        "\n",
        "print(pct_daily_return)\n",
        "print()\n",
        "print(std_dev)"
