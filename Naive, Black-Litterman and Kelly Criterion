        "port_returns, port_volatility, sharpe_ratio, portfolio_weights = monte_carlo_simulation(returns, num_portfolios)\n",
        "\n",
        "# Plotting the efficient frontier\n",
        "optimal_weights, optimal_mus, optimal_sigmas = calculate_frontier(returns)\n",
        "plt.plot(optimal_sigmas, optimal_mus, 'y-o', markersize=8, label='Efficient Frontier')\n",
        "plt.scatter(port_volatility, port_returns, c=sharpe_ratio, cmap='viridis', marker='o')\n",
        "plt.colorbar(label='Sharpe Ratio')\n",
        "plt.xlabel('Expected Volatility')\n",
