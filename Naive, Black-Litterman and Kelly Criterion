      "source": [
        "#### Monte Carlo simulation"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "-q8o2ky_6B1-"
      },
      "outputs": [],
      "source": [
        "def monte_carlo_c(returns_df, num_trials):\n",
        "    annual_return = returns.sum().values / 3 # time frame is 3 years\n",
        "    annual_vol = returns.std().values * np.sqrt(52) # 52 weeks in one year\n",
        "\n",
        "    weights = np.random.rand(num_trials, len(tickers))\n",
        "    print(weights.shape)\n",
        "    weights = np.vstack((weights,\n",
        "                         np.array([[0.5465, 0.0, 0.0, 0.3084, 0.1451]]))) # Markowitz\n",
        "    weights = weights / weights.sum(axis=1, keepdims=True) # normalize weights\n",
        "\n",
        "    mc_returns = weights @ annual_return\n",
        "    mc_vol = weights @ annual_vol\n",
        "\n",
        "    return (mc_returns, mc_vol)"
      ]
    },
