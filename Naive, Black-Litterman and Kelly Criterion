      },
      "source": [
        "#### Monte Carlo simulation"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "sDst3Lno6B1-"
      },
      "outputs": [],
      "source": [
        "def monte_carlo_simulation(returns, num_portfolios):\n",
        "    port_returns = []\n",
        "    port_volatility = []\n",
        "    sharpe_ratio = []\n",
        "    portfolio_weights = []\n",
        "\n",
        "    for _ in range(num_portfolios):\n",
        "        weights = np.random.random(n_assets)\n",
        "        weights /= np.sum(weights)\n",
        "\n",
        "        portfolio_return = np.sum(returns.mean() * weights) * 52  # annualized return\n",
        "        portfolio_std_dev = np.sqrt(np.dot(weights.T, np.dot(returns.cov() * 52, weights)))  # annualized volatility\n",
        "\n",
        "        port_returns.append(portfolio_return)\n",
        "        port_volatility.append(portfolio_std_dev)\n",
        "        sharpe_ratio.append(portfolio_return / portfolio_std_dev)\n",
        "        portfolio_weights.append(weights)\n",
        "\n",
        "    return port_returns, port_volatility, sharpe_ratio, portfolio_weights\n"
      ]
    },
    {
